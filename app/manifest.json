{
  "name": "__MSG_appName__",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "devtools_page": "chrome_components/devtools/devtools.html",
  "icons": {
    "16": "chrome_components/images/code-16.png",
    "128": "chrome_components/images/code-128.png"
  },
  "default_locale": "en",
  "options_page": "chrome_components/options/options.html",
  "background": {
    "scripts": [
      "chrome_components/background/chromereload.js",
      "chrome_components/background/background.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage"
  ]
}
